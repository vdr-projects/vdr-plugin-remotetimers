msgid "Show progress bar in menu \"%s\""
msgstr "Mostra barra avanzamento nel menu \"%s\""

#, c-format
msgid "User ID filter in menu \"%s\""
msgstr "Filtra per ID utente nel menu \"%s\""

msgid "Remote recordings"
msgstr "Registrazioni remote"

